{
  "name": "@jamespatrickwhite/utilityurls",
  "version": "0.7.0",
  "description": "A collection of utility URL classes that simplify configuration details",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "c8 tap --no-coverage",
    "build:docs": "npm run build:docs:md && npm run build:docs:html",
    "build:docs:md": "jsdoc2md index.js *URL.js > API.md",
    "build:docs:html": "jsdoc -d docs --readme README.md index.js *URL.js"
  },
  "keywords": [
    "utility",
    "url",
    "iot"
  ],
  "author": "James White",
  "license": "MIT",
  "homepage": "https://github.com/jamespatrickwhite/utilityurls",
  "bugs": {
    "url": "https://github.com/jamespatrickwhite/utilityurls/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamespatrickwhite/utilityurls.git"
  },
  "devDependencies": {
    "c8": "^7.10.0",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.1.0",
    "tap": "^15.1.5"
  }
}
